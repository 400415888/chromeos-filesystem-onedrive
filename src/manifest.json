{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "__MSG_shortName__",
  "description": "__MSG_appDescription__",
  "version": "3.0.0.36170",
  "version_name": "3.0.0 Alpha - Build 36170",
  "default_locale": "en",
  "permissions": [
